<% user_id = current_user.id %>
<%= form_with(model: @list, local: true) do |form| %>

<div class="field">
    <%= form.label :list_title %>
    <%= form.text_field :list_title %>
</div>

<div class="field">
    <%= form.label :musical %>
    <%= form.select :musical_id, options_from_collection_for_select(Musical.all, 'id', 'title') %>
</div> 

<%= form.submit %>

<% end %>
