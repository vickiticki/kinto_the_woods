<%= form_with(model: @listed_musicals) do |form| %>

<div class="field">
    <%= form.label :musical %>
    <%= form.select :musical_id, options_from_collection_for_select(Musical.all, 'id', 'title') %>
</div> 

<div class="field">
    <%= form.label :list %>
    <%= form.select :list, options_from_collection_for_select(List.all, 'id', 'name') %>
</div> 

<%= form.submit %>

<% end %>